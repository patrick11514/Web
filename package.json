{
    "name": "web",
    "private": true,
    "version": "0.0.1",
    "type": "module",
    "scripts": {
        "dev": "vite dev",
        "build": "vite build",
        "preview": "vite preview",
        "prepare": "svelte-kit sync || echo ''",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "format": "prettier --write .",
        "lint": "prettier --check . && eslint .",
        "genDatabaseSchema": "kysely-codegen --out-file ./src/types/database.ts",
        "start": "node -r dotenv/config build",
        "migration:create": "./script/createMigration.sh",
        "migrate": "tsx ./script/runMigration.ts"
    },
    "devDependencies": {
        "@eslint/compat": "^1.2.5",
        "@eslint/js": "^9.18.0",
        "@sveltejs/adapter-node": "^5.2.9",
        "@sveltejs/kit": "^2.16.0",
        "@sveltejs/vite-plugin-svelte": "^5.0.0",
        "@tailwindcss/typography": "^0.5.15",
        "@tailwindcss/vite": "^4.0.0",
        "@types/jsonwebtoken": "^9.0.7",
        "@types/uuid": "^10.0.0",
        "eslint": "^9.18.0",
        "eslint-config-prettier": "^10.0.1",
        "eslint-plugin-svelte": "^3.0.0",
        "globals": "^16.0.0",
        "kysely-codegen": "^0.17.0",
        "prettier": "^3.4.2",
        "prettier-plugin-svelte": "^3.3.3",
        "prettier-plugin-tailwindcss": "^0.6.11",
        "svelte": "^5.0.0",
        "svelte-check": "^4.0.0",
        "tailwindcss": "^4.0.0",
        "typescript": "^5.0.0",
        "typescript-eslint": "^8.20.0",
        "vite": "^6.2.6"
    },
    "dependencies": {
        "@patrick115/sveltekitapi": "^1.2.12",
        "bootstrap-icons": "^1.11.4",
        "dotenv": "^16.4.5",
        "jsonwebtoken": "^9.0.2",
        "kysely": "^0.27.4",
        "mysql2": "^3.11.4",
        "simple-json-db": "^2.0.0",
        "tailwind-merge": "^3.2.0",
        "uuid": "^11.0.2",
        "zod": "^3.23.8"
    }
}

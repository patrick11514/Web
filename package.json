        "svelte": "^5.14.5",
        "svelte-check": "^4.1.1",
        "tailwindcss": "^3.4.17",
        "tslib": "^2.8.1",
        "typescript": "^5.7.2",
        "typescript-eslint": "^8.18.1",
        "vite": "^6.0.4"

{
    "name": "mainweb",
    "version": "0.0.5",
    "private": true,
    "scripts": {
        "dev": "vite dev",
        "build": "vite build",
        "preview": "vite preview",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "lint": "prettier --plugin prettier-plugin-svelte --plugin prettier-plugin-tailwindcss --check . && eslint .",
        "format": "prettier --plugin prettier-plugin-svelte --plugin prettier-plugin-tailwindcss --write .",
        "genDatabaseSchema": "kysely-codegen --out-file ./src/types/database.ts",
        "start": "node -r dotenv/config build"
    },
    "devDependencies": {
        "@sveltejs/adapter-node": "^5.2.0",
        "@sveltejs/kit": "^2.5.17",
        "@sveltejs/vite-plugin-svelte": "^3.1.1",
        "@types/bcrypt": "^5.0.2",
        "@types/eslint": "8.56.10",
        "@types/jsonwebtoken": "^9.0.6",
        "@types/path-browserify": "^1.0.2",
        "@types/uuid": "^10.0.0",
        "@typescript-eslint/eslint-plugin": "^7.13.1",
        "@typescript-eslint/parser": "^7.13.1",
        "autoprefixer": "^10.4.19",
        "eslint": "^9.5.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-svelte": "^2.40.0",
        "kysely-codegen": "^0.15.0",
        "postcss": "^8.4.38",
        "prettier": "^3.3.2",
        "prettier-plugin-svelte": "^3.2.5",
        "prettier-plugin-tailwindcss": "^0.6.5",
        "svelte": "^4.2.18",
        "svelte-check": "^3.8.1",
        "tailwindcss": "^3.4.4",
        "tslib": "^2.6.3",
        "typescript": "^5.5.2",
        "vite": "^5.3.1"
    },
    "type": "module",
    "dependencies": {
        "@patrick115/sveltekitapi": "^1.2.10",
        "bcrypt": "^5.1.1",
        "bootstrap-icons": "^1.11.3",
        "dompurify": "^3.1.5",
        "dotenv": "^16.4.5",
        "jsonwebtoken": "^9.0.2",
        "kysely": "^0.27.3",
        "mysql2": "^3.10.1",
        "path-browserify": "^1.0.1",
        "sharp": "^0.33.4",
        "simple-json-db": "^2.0.0",
        "sweetalert2": "^11.12.0",
        "tailwind-merge": "^2.3.0",
        "uuid": "^10.0.0",
        "zod": "^3.23.8"
    }
}
